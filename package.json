{
  "name": "article-collector",
  "version": "1.0.0",
  "description": "this program can collect the content of a article on medium published",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start-server": "node src/server/index.js",
    "start": "npm-run-all -s -c build start-server",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint src",
    "test-report": "mocha __test__/spec.js --reporter reporter.js --exit",
    "test": "mocha __test__/spec.js --exit",
    "pre-test": "node __test__/preTest.js",
    "check": "node .airci/submit.js",
    "submit": "npm-run-all -s --silent pre-test check"
  },
  "author": "Code States",
  "license": "",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "jsdom": "^13.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.3.3",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "mock-fs": "^4.10.4",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.0",
    "supertest": "^3.3.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
